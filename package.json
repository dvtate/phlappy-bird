{
  "name": "phlappy-bird",
  "version": "1.0.0",
  "description": "simple flappy-bird clone demo that uses postfix haskell and wasm",
  "main": "index.js",
  "scripts": {
    "build": "phc file ./src/main.phs -t0 > ./dist/game.wat && wat2wasm ./dist/game.wat -o ./dist/game.wasm",
    "start": "http-server",
    "optimize": "wasm-opt ./dist/game.wasm -o ./dist/game.wasm -O3 && wasm2wat ./dist/game.wasm > ./dist/game.wat"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dvtate/breakout"
  },
  "author": "dvtate",
  "license": "MIT",
  "devDependencies": {
    "postfix-haskell": "^0.0.6",
    "wat2wasm": "^3.0.0"
  },
  "dependencies": {
    "http-server": "^13.0.2"
  }
}
